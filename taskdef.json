{
    "family": "ecs-test-dev",
    "executionRoleArn": "arn:aws:iam::788601987176:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "ecs-test-dev",
            "image": "amazon/amazon-ecs-sample",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ],
            "environment": [
                {
                    "name": "var",
                    "value": "value"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512"
}